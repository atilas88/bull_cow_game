{
    "openapi": "3.0.0",
    "info": {
        "title": "Bulls and Cows game",
        "description": "Game API",
        "version": "0.0.1"
    },
    "paths": {
        "/api/game/create": {
            "post": {
                "tags": [
                    "Game"
                ],
                "summary": "Game create",
                "operationId": "ac0c37c02739b99cedc0f4fabc868823",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/x-www-form-urlencoded": {
                            "schema": {
                                "required": [
                                    "username",
                                    "user_age"
                                ],
                                "properties": {
                                    "username": {
                                        "description": "User name",
                                        "type": "string"
                                    },
                                    "user_age": {
                                        "description": "User age",
                                        "type": "integer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Game created"
                    },
                    "400": {
                        "description": "Input erros"
                    },
                    "default": {
                        "description": "An error occurred"
                    }
                }
            }
        },
        "/api/game/delete/{id}": {
            "delete": {
                "tags": [
                    "Game"
                ],
                "summary": "Delete a game",
                "operationId": "f104f44523cfaba739c8966756d3be5d",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "examples": {
                            "int": {
                                "summary": "Enter a game id",
                                "value": "1"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Game delete successfully"
                    },
                    "404": {
                        "description": "The game has been not found"
                    },
                    "default": {
                        "description": "An error occurred"
                    }
                }
            }
        }
    }
}